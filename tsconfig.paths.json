{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@api": ["src/api"],
      "@api/*": ["src/api/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@reduce": ["src/reduce"],
      "@reduce/*": ["src/reduce/*"],
      "@type": ["src/types"],
      "@type/*": ["src/types/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
